<div class="yui-g">
  <h1>Edit Size</h1>
  <div id="size">
    <%= error_messages_for :size %>
    <% form_for :size, @size, :url => admin_size_path(@size), :html => { :method => :put } do |f| %>
      <%= render :partial => 'admin/sizes/form', :locals => { :f => f } %>
      <br/>
      <%= submit_tag :update %>
    <% end %>
  </div>
</div>
